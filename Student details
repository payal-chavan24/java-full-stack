//using data types declare student details in varriable and print it 
public class Studentinfo {

	public static void main(String[] args) {
		String studentname="Payal bhanudas chavan";
		byte rollno=66;
		short idno=34;
		long mobileNO=932345678L;
		String address ="Kalbhurgi,karnataka";
		char gender='f'; 
		int entranceExamMarks=135;
		boolean result=true;
		float marksin10 =78.40f;
		double marksin12=92.00;
		int passingyear=2025;
		String course="B.sc in (computer science)";
		int totalfees=2500;
		String caste="lambani";
		String dob="24 may 2003";
		byte duration=3;
		
	    System.out.println("Student details");
		System.out.println("Name of the student="+studentname);
		System.out.println("Roll no="+rollno);
		System.out.println("ID no.="+idno);
		System.out.println("Mobile no.="+mobileNO);
		System.out.println("Address="+address );
        System.out.println("Gender="+gender);
        System.out.println("Entrance exam="+entranceExamMarks+"marks");
        System.out.println("Result="+result);
        System.out.println("Marks obtained in class 10="+marksin10+"%");
        System.out.println("marks obtained in class 12="+marksin12+"%");
        System.out.println("Passing year="+passingyear);
        System.out.println("Course="+course);
        System.out.println("Total fees="+totalfees);
        System.out.println("date of birth="+dob);
        System.out.println("Duration of couse="+duration+"years");
        System.out.println("Caste="+caste);
	}

	
}
