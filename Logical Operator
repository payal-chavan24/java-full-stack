
public class LogicalOperator {

	public static void main(String[] args) {
		int a=7,b=10;
		System.out.println((a<=b)||!(a==b)&&!(a==b));
		// T || T && T=T                
		System.out.println(!(a>=b)&&!(a!=b)&&!(a<b));
		//T&&F&&F=F
		System.out.println((a<b)||!(a<=b)&&!(a>a));
		//T||F&&T=T
		System.out.println((a<b)&&!(a!=b)||(a==b));
        //T&&F||F=F 
	}

}
