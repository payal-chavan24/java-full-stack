
public class ORoperator {

	public static void main(String[] args) {
		int a=7,b=60;
		System.out.println("Logical Operators- Or (||) Operator");
		System.out.println((a>b)||(a<b));
		System.out.println((a>=b)||(a<=b));
		System.out.println((a==b)||(a!=b));
		System.out.println((a==b)||(a>b));
		
	}

}
