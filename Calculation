package calculator;
package calculator;

public class Calc {

	public static void main(String[] args) {

		System.out.println("Addition=" + (22 + 4));
		System.out.println("Substraction=" + (22 - 4));
		System.out.println("Divide=" + 22 / 4);
		System.out.println("Module=" + (22 % 4));
		System.out.println("Multiply=" + (22 * 4));

	}

}


